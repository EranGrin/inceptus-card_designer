        <field name="key">svg_margin_bottom</field>
        <field name="value">0in</field>
    </record>

    <record id="svg_margin_left" model="ir.config_parameter">
        <field name="key">svg_margin_left</field>
        <field name="value">0in</field>
    </record>

    <record id="svg_file_name" model="ir.config_parameter">
        <field name="key">svg_file_name</field>
        <field name="value">card_design</field>
    </record>
</odoo>